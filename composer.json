{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.favorit:integrations/ihub-grid/packages/dynamic-scheduler.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.favorit:integrations/ihub-grid/packages/accounting.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.favorit:integrations/ihub-grid/packages/error-handler.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.favorit:integrations/ihub-grid/packages/seamless-wallet-core.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.favorit:integrations/ihub-grid/packages/seamless-wallets/euro-games-tech.git"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.4.*",
        "ihub-grid/packages/dynamic-scheduler": "^1.0",
        "ihub-grid/packages/accounting": "^1.0",
        "ihub-grid/packages/error-handler": "^1.0",
        "ihub-grid/packages/seamless-wallet-core": "^1.1",
        "ihub-grid/packages/seamless-wallets/euro-games-tech": "^1.0",
        "predis/predis": "~1",
        "filp/whoops": "~1",
        "kozz/laravel-guzzle-provider": "~6",
        "Nathanmac/Parser": "~4",
        "spatie/laravel-fractal": "~1",
        "danielstjules/stringy": "~2",
        "doctrine/dbal": "~2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1",
        "mockery/mockery": "~0",
        "codeception/codeception": "~2",
        "codeception/specify": "~0",
        "codeception/verify": "~0",
        "symfony/css-selector": "~3",
        "symfony/dom-crawler": "~3",
        "barryvdh/laravel-ide-helper": "~2",
        "flow/jsonpath": "~0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Codeception\\": "app/Components/Codeception",
            "Testing\\": "tests/_support/Mocks"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan vendor:publish --tag=tests --force",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan vendor:publish --tag=tests --force",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
